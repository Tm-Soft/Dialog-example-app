<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:gravity="center">


    <Button
        android:id="@+id/btn_one_button_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="One Button Dialog"
        android:layout_gravity="center"/>


    <Button
        android:id="@+id/btn_two_button_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Two Button Dialog"
        android:layout_gravity="center"/>

    <Button
        android:id="@+id/btn_input_string_button_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EditText Button Dialog"
        android:layout_gravity="center"/>


    <Button
        android:id="@+id/btn_select_list_button_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Select List Button Dialog"
        android:layout_gravity="center"/>

    <Button
        android:id="@+id/btn_kakao_login_button_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kakao Login Button Dialog"
        android:layout_gravity="center"/>



</LinearLayout>